  "main": "dist/game-store.js",
  "module": "dist/game-store.esm.js",
  "types": "dist/game-store.d.ts",
  "scripts": {
    "test": "jest test/*.spec.ts --notify --config=jest.config.js",
    "cov": "jest test/*.spec.ts --notify --config=jest.config.js --coverage",
    "build": "rimraf dist types && webpack --config webpack.config.js",
    "dev": "node scripts/server.js",
    "dts": "rimraf dist/game-store.d.ts && dts-generator --name game-store --project . --out dist/game-store.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RequireSun/game-store.git"
  },
  "author": "",
