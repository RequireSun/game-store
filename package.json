  "bugs": {
    "url": "https://github.com/RequireSun/game-store/issues"
  },
  "homepage": "https://github.com/RequireSun/game-store#readme",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "webpack-dev-server": "^3.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/es6-promise": "^3.3.0",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.11.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0-beta.6",
    "dts-generator": "^2.1.0",
    "es6-promise": "^4.2.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "open-browser-webpack-plugin": "0.0.5",
    "redux-actions": "^2.3.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.2",
    "ts-loader": "^5.2.1",
    "typescript": "^3.1.1",
    "vue-own-redux": "^0.2.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
